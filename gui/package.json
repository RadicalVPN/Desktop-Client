{
  "name": "radicalvpn-desktop",
  "description": "Radical VPN Desktop Client",
  "version": "0.0.1",
  "author": "RadicalVPN",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "dev": "vite",
    "build": "npm run lint && vue-tsc --noEmit && vite build && electron-builder",
    "build:ci": "vite build",
    "start:ci": "serve -s ./dist",
    "prelint": "npm run format:build",
    "lint": "eslint  \"./src/**/*.{ts,js,vue}\"",
    "lint:fix": "eslint --fix \"./src/**/*.{ts,js,vue}\"",
    "format": "prettier --write .",
    "format:build": "prettier --check .",
    "preview": "vite preview"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "lint-staged": {
    "./src/**/*.{ts,js,vue}": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "@amcharts/amcharts5": "^5.5.6",
    "@amcharts/amcharts5-geodata": "^5.1.1",
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@vueuse/core": "^10.6.1",
    "axios": "^1.6.1",
    "chart.js": "^3.8.0",
    "epic-spinners": "^2.0.0",
    "flag-icons": "^6.15.0",
    "pinia": "^2.1.7",
    "sass": "^1.69.5",
    "serve": "^14.2.1",
    "vue": "^3.3.8",
    "vue-chartjs": "^4.1.1",
    "vue-i18n": "^9.6.5",
    "vue-router": "^4.2.5",
    "vuestic-ui": "^1.7.3"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.12.2",
    "@types/leaflet": "^1.9.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vitejs/plugin-vue": "^4.4.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "autoprefixer": "^10.4.16",
    "electron": "^27.1.2",
    "electron-builder": "^24.6.4",
    "eslint": "^8.13.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.0.1",
    "postcss": "^8.4.31",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.3.5",
    "typescript": "^4.5.4",
    "vite": "^4.5.0",
    "vite-plugin-electron": "^0.15.4",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue-eslint-parser": "^8.3.0",
    "vue-tsc": "^1.8.22"
  }
}
